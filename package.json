{
  "name": "nope-rule",
  "module": "app/index.ts",
  "type": "module",
  "scripts": {
    "compile": "bun build --compile --minify --sourcemap --bytecode ./app/index.ts",
    "compile:windows": "bun run compile --target=bun-windows-x64 --outfile out/app-windows.exe",
    "compile:linux": "bun run compile --target=bun-linux-x64 --outfile out/app-linux",
    "compile:linux:arm": "bun run compile --target=bun-linux-arm64 --outfile out/app-linux-arm",
    "compile:darwin": "bun run compile --target=bun-darwin-arm64 --outfile out/app-darwin",
    "compile:darwin:arm": "bun run compile --target=bun-darwin-arm64 --outfile out/app-darwin-arm",
    "compile:all": "bun run compile:windows && bun run compile:linux && bun run compile:linux:arm && bun run compile:darwin && bun run compile:darwin:arm",
    "start": "bun run app/index.ts"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "jsep": "^1.4.0",
    "json-logic-engine": "^5.0.2"
  }
}
